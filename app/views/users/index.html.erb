<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<% @users.each do |user| %>
   <%= user.name %><br />
   <%= link_to "編集",edit_user_path(user.id) %>
   <%= link_to "削除",user_path(user.id), method: :delete, data:{confirm: "本当によろしいでしょうか？"} %>
<% end %>
<p><%= link_to '管理画面に戻る',v2s_index_path %></p>
